{
  "expo": {
    "name": "Ecommerce App",
    "slug": "ecommerce-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
